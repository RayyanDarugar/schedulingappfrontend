    "            <i class=\"fas fa-angle-right next\"></i>\n",
    "          </div>\n",
    "          <div class=\"weekdays\">\n",
    "            <div>Sun</div>\n",
    "            <div>Mon</div>\n",
    "            <div>Tue</div>\n",
    "            <div>Wed</div>\n",
    "            <div>Thu</div>\n",
    "            <div>Fri</div>\n",
    "            <div>Sat</div>\n",
    "          </div>\n",
    "          <div class=\"days\"></div>\n",
    "          <div class=\"goto-today\">\n",
    "            <div class=\"goto\">\n",
    "              <input type=\"text\" placeholder=\"mm/yyyy\" class=\"date-input\" />\n",
    "              <button class=\"goto-btn\">Go</button>\n",
    "            </div>\n",
    "            <button class=\"today-btn\">Today</button>\n",
    "          </div>\n",
    "        </div>\n",
    "      </div>\n",
    "      <div class=\"right\">\n",
